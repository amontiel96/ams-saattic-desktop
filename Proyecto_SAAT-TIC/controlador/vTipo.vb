Public Class vTipo
    Dim vTipo, vId As String

    Public Property gTipo
        Get
            Return vTipo
        End Get
        Set(value)
            vTipo = value
        End Set
    End Property

    Public Property gId
        Get
            Return vId
        End Get
        Set(value)
            vId = value
        End Set
    End Property



End Class
